MOV EAX EMA
MOV EBX 0x00

;; Fill with red
RED:
ADD EBX 0x01
MEM EAX 0x01
ADD EAX 0x01
CMP EBX 0x100
JNE RED

;; Green line
MOV EAX EMA
ADD EAX 0x0f
MOV EBX 0x00
GREEN:
MEM EAX 0x02
ADD EAX 0x0f
ADD EBX 0x01
CMP EBX 0x10
JNE GREEN

;; Blue line
MOV EAX EMA
MOV EBX 0x00
BLUE:
MEM EAX 0x03
ADD EAX 0x11
ADD EBX 0x01
CMP EBX 0x10
JNE BLUE
